public String ageGroup(Integer n) {
    String  grupo;
    if( (n>=0) && (n<=1) ){
        return grupo = 'Infant';
    } else if ( (n>=2) && (n<=14) ){
        return grupo = 'Child';
    } else if ( (n>=15) && (n<=21) ){
        return grupo = 'Youth';
    } else {
        return grupo = 'Adult';
    }
}